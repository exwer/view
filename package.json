{
  "name": "view",
  "version": "0.0.0",
  "packageManager": "pnpm@6.32.3",
  "description": "",
  "author": "love-JS <ss.lbw.ss@gmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/love-js",
  "homepage": "https://github.com/love-js/view#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/love-js/view.git"
  },
  "bugs": {
    "url": "https://github.com/love-js/view/issues"
  },
  "keywords": [
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./lib/index.mjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./lib/*",
        "./lib/index.d.ts"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "addExample": "esno ./scripts/addExample.ts",
    "preinstall": "npx only-allow pnpm",
    "build": "unbuild",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.1",
    "@babel/types": "^7.18.4",
    "@types/node": "^17.0.45",
    "bumpp": "^7.2.0",
    "eslint": "^8.18.0",
    "esno": "^0.16.3",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.18.0"
  }
}
