{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "lib": ["es6", "DOM"],
    "downlevelIteration": true,
    "moduleResolution": "node",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}
